{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "three-js-documentation",
  "title": "Three.js documentation",
  "description": "Search and display documention for Three.js",
  "icon": "command-icon.png",
  "author": "sibhod",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search the Three.js documentation",
      "description": "Search the Three.js documentation and display reference results in Raycast, open the documentation URL, or open the file in GitHub.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.3",
    "@raycast/utils": "^1.4.8",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "preferences": [
    {
      "name": "language",
      "title": "Language",
      "description": "The api documentation language.",
      "type": "dropdown",
      "default": "en",
      "required": false,
      "data": [
        {
          "title": "English",
          "value": "en"
        },
        {
          "title": "Arabic",
          "value": "ar"
        },
        {
          "title": "French",
          "value": "fr"
        },
        {
          "title": "Italian",
          "value": "it"
        },
        {
          "title": "Japanese",
          "value": "jp"
        },
        {
          "title": "Korean",
          "value": "ko"
        },
        {
          "title": "Portuguese",
          "value": "pt-br"
        },
        {
          "title": "Russian",
          "value": "ru"
        },
        {
          "title": "Chinese (Simplified)",
          "value": "zh"
        }
      ]
    },
    {
      "name": "release",
      "title": "Release Version",
      "description": "The release version of the documentation, with fallback to 'latest'.",
      "default": "latest",
      "type": "textfield",
      "required": false,
      "placeholder": "latest"
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
